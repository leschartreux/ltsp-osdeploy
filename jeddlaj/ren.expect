set netclnt_program [lindex $argv 0]
set server [lindex $argv 1]
set passwd [lindex $argv 2]

set path  [lindex $argv 3]
set file  [lindex $argv 4]
set newname  [lindex $argv 5]

spawn $netclnt_program 
expect "Netclnt"
send  "open $server\r"
expect -exact "Password:"
send  "$passwd\r"
expect -exact "NETFS> "
send "cd $path\r"
expect -exact "NETFS> "
send "ren $file $newname\r"
expect -exact "NETFS> "
send "exit\r"
