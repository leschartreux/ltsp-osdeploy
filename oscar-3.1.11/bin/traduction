#----------------------------------------------------------------------------------------------------
#----------------------------------------------------------------------------------------------------
main()
{
if ( test -e /usr/share/oscar/usr/choix_langue )
then
	choix_langue=`cat /usr/share/oscar/usr/choix_langue`
else
	echo "fr" > /usr/share/oscar/usr/choix_langue
	choix_langue=fr
fi

case "$choix_langue" in
	fr)
		if ! ( test -e /usr/share/oscar/usr/langue/fr )
		then
			mkdir -p /usr/share/oscar/usr/langue/fr
		fi
		chemin_langue=/usr/share/oscar/usr/langue/fr
		;;
	it)
		if ! ( test -e /usr/share/oscar/usr/langue/it )
		then
			mkdir -p /usr/share/oscar/usr/langue/it
		fi
		chemin_langue=/usr/share/oscar/usr/langue/it
		;;
	de)
		if ! ( test -e /usr/share/oscar/usr/langue/de )
		then
			mkdir -p /usr/share/oscar/usr/langue/de
		fi
		chemin_langue=/usr/share/oscar/usr/langue/de
		;;
	us)
		if ! ( test -e /usr/share/oscar/usr/langue/us )
		then
			mkdir -p /usr/share/oscar/usr/langue/us
		fi
		chemin_langue=/usr/share/oscar/usr/langue/us
		;;
	es)	# plus tard ...
		if ! ( test -e /usr/share/oscar/usr/langue/es )
		then
			mkdir -p /usr/share/oscar/usr/langue/es
		fi
		chemin_langue=/usr/share/oscar/usr/langue/es
		;;
esac
}
#----------------------------------------------------------------------------------------------------
PATH="/usr/share/oscar/bin:$PATH"
/usr/share/oscar/usr/version_theme_oscar
main
traduire_textes
