#----------------------------------------------------------------------------------------------------
choix_aide_serveur_nfs()
{
dialog  --no-cancel --colors --no-shadow --backtitle "`cat /etc/banniere_oscar`" --title " Synoptiques d'installation des clients " \
	--menu  "\n\Zb\Z3     Sélectionnez la procédure à visualiser :\n\n" 10 80 0  \
	" " "" \
	restaure "\Zb\Z3Restaure\Z4 les postes clients à partir du poste serveur." \
	simple "\Zb\Z3Restaure\Z4 à partir du serveur puis \Zb\Z3sauvegarde\Z4 les clients. " \
	formate "\Zb\Z3Formate\Z4 identique au serveur puis \Zb\Z3restaure\Z4 les clients." \
	complète "\Zb\Z3Formate\Z4, \Zb\Z3restaure\Z4 puis \Zb\Z3sauvegarde\Z4 les clients." \
	" " "" \
	table "\Zb\Z3Envoie\Z4 la table des partitions puis \Zb\Z3formate\Z4 les clients." \
	" " "" \
	quitter "\Z1Quitter\Zb\Z4 ces commandes." \
	" " "" 2>/tmp/tempfile

output=`cat /tmp/tempfile`

if [ "$output" = " " ]
then
	choix_aide_serveur_nfs
	return
elif [ "$output" = "restaure" ]
then
	touche_continuer=`cat $chemin_langue/Continuer`
	touche_quitter="--no-cancel"
	serveur_client_nfs
elif [ "$output" = "simple" ]
then
	touche_continuer=`cat $chemin_langue/Continuer`
	touche_quitter="--no-cancel"
	serveur_client_nfs_sauve
elif [ "$output" = "formate" ]
then
	sous_programme_formate_restaure
elif [ "$output" = "complète" ]
then
	sous_programme_formate_restaure_sauve
elif [ "$output" = "table" ]
then
	touche_continuer=`cat $chemin_langue/Continuer`
	touche_quitter="--no-cancel"
	titre_serveur=Serveur_image
	titre_procedure="Procédure serveur nfs formate"
	texte_titre="Procédure serveur nfs formate"
	commande_Continuer=return
	serveur_client_formate_seul
elif [ "$output" = "quitter" ]
then
	rm -f /tmp/tempfile
	exit
else
    choix_aide_serveur_nfs
    return
fi
rm -f /tmp/tempfile
choix_aide_serveur_nfs
}
#----------------------------------------------------------------------------------------------------
choix_aide_serveur_synchrone()
{
dialog  --no-cancel --colors --no-shadow --backtitle "`cat /etc/banniere_oscar`" --title " Synoptiques d'installation des clients en mode synchrone " \
	--menu  "\n\Zb\Z3     Sélectionnez la procédure en synchrone à visualiser :\n\n" 10 80 0  \
	" " "" \
	simple "\Zb\Z4Envoie sa \Z3sauvegarde\Z4 aux postes clients puis les \Zb\Z3restaure\Z4." \
	complète "\Zb\Z3Formate\Z4 les clients, envoie sa \Zb\Z3sauvegarde\Z4 puis les \Zb\Z3restaure\Z4." \
	" " "" \
	disque "\Zb\Z3copie\Z4 un disque du serveur sur les postes clients. " \
	table "\Zb\Z4Envoie la \Z3table\Z4 des partitions puis \Zb\Z3formate\Z4 les clients." \
	" " "" \
	quitter "\Z1Quitter\Zb\Z4 ces commandes." \
	" " "" 2>/tmp/tempfile

output=`cat /tmp/tempfile`

if [ "$output" = " " ]
then
	choix_aide_serveur_synchrone
	return
elif [ "$output" = "simple" ]
then
	aide_serveur serveur_client_multi
elif [ "$output" = "petite" ]
then
	aide_serveur serveur_client_multi_petite
elif [ "$output" = "complète" ]
then
	aide_serveur serveur_client_clone
elif [ "$output" = "disque" ]
then
	aide_serveur serveur_client_disque
elif [ "$output" = "table" ]
then
	aide_serveur serveur_client_table
elif [ "$output" = "===formate" ]
then
	touche_continuer=`cat $chemin_langue/Continuer`
	touche_quitter="--no-cancel"
	titre_serveur=Serveur_image
	titre_procedure="Procédure serveur nfs formate"
	texte_titre="Procédure serveur nfs formate"
	commande_Continuer=return
	serveur_client_formate_seul
elif [ "$output" = "quitter" ]
then
	rm -f /tmp/tempfile
	exit
else
    choix_aide_serveur_synchrone
    return
fi
choix_aide_serveur_synchrone
}
