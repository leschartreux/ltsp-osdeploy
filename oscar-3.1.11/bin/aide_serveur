cp /tmp/$text_image_clone /tmp/text_image
output=exit

dialog  $touche_quitter --ok-label "$touche_continuer" $touche_extra --colors --no-shadow \
        --backtitle "`cat /etc/banniere_oscar`" --title "$texte_titre" \
        --menu "`cat /tmp/text_image` " 0 0 0 \
        "$etape" "" # 2>/tmp/tempfile

output=$?

if [ "$output" = "255" ]
then
    rm -f /tmp/text_image
    exit
elif [ "$output" = "1" ]
then
    rm -f /tmp/text_image
    exit
elif [ "$output" = "0" ]
then
    rm -f /tmp/text_image
    return
elif [ "$output" = "3" ]
then
    sous_programme_formate_restaure
    rm -f /tmp/text_image
    return
fi
}
