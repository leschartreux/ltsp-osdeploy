PATH="/usr/share/oscar/bin:$PATH"



if ( test -e /tmp/souris_installee )
then
cp -f /etc/init.d/gpm_bon /etc/init.d/gpm
/etc/init.d/gpm stop
rm /tmp/souris_installee
fi