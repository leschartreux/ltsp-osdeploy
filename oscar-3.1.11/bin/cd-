# démonte le cdrom


umount /mnt/cdrom  ; sync
eject

if ! ( test -e /etc/cdrom_ejecte )
then
	if ( test -e /etc/demarrage_cdcache )
	then
		echo "" > /etc/cdrom_ejecte	# cd ejecte
	fi
fi

echo
echo -e "Pour monter le cdrom, \033[1;33mcd+\033[1;m"
 texte_fin