<?xml version="1.0"?>

<JWM>

	<RootMenu height="20" onroot="123">
		<Menu icon="folder_yellow.png" label="System">
			<Program icon="/usr/share/oscar/icones/kcmpartitions.png" label="GParted">gparted</Program>
			<Program icon="partimage.png" label="Partimage">terminal -e /usr/sbin/partimage</Program>
			<Program icon="testdisk.png" label="Testdisk">terminal -e /usr/sbin/testdisk</Program>
			<Program icon="/usr/share/oscar/icones/pengupetit.png" label="OSCAR">terminal -e /usr/share/oscar/bin/oscar</Program>
		</Menu>

		<Menu icon="folder_yellow.png" label="Misc">
			<Program icon="" label="Midnight Commander">terminal -e /usr/bin/mc</Program>
			<Program icon="" label="Epdfview PDF viewer">epdfview</Program>
			<Program icon="" label="Dillo web browser">dillo</Program>
		</Menu>

		<Separator/>
			<Program icon="/usr/share/oscar/icones/pengupetit.png" label="OSCAR">terminal -e /usr/share/oscar/bin/oscar</Program>
			<Program icon="/usr/share/oscar/icones/kcmpartitions.png" label="GParted">gparted</Program>
			<Program icon="/usr/share/oscar/icones/kfm_home.png" label="mc">terminal -e mc</Program>
			<Program icon="/usr/share/oscar/icones/vnc64.png" label="Vnc">vncviewer</Program>
			<Program icon="xterm.png" label="Terminal">terminal</Program>
			<Program icon="xfburn.png" label="Graveur CD/DVD">xfburn</Program>
			<Program icon="geany.png" label="Geany editor">geany</Program>
			<Program icon="vim.png" label="GVim">gvim</Program> 
			<Program icon="firefox.png" label="Firefox">firefox</Program>
			<Program icon="/usr/share/oscar/icones/postscript-viewer.png" label="Epdfview">epdfview</Program>
		<Separator/>
		
		<Menu icon="folder_yellow.png" label="Quitter">
			<Program icon="" label="Quitter">killall jwm</Program>
			<Restart icon="" label="Relancer Window Manager"/>
		</Menu>
	</RootMenu>
	

	<Tray x="0" y="0" height="40">
		<TrayButton popup="Menu" icon="/usr/share/oscar/icones/packageutilities.png">root:1</TrayButton>
		<TrayButton popup="Bureau" icon="desktop.png">showdesktop</TrayButton>
		<TrayButton popup="OSCAR" icon="/usr/share/oscar/icones/pengupetit.png">exec:terminal -e /usr/share/oscar/bin/oscar</TrayButton>
		<TrayButton popup="GParted" icon="/usr/share/oscar/icones/kcmpartitions.png">exec:gparted</TrayButton>
		<TrayButton popup="mc" icon="/usr/share/oscar/icones/kfm_home.png">exec:terminal -e mc</TrayButton>
		<TrayButton popup="Vnc" icon="/usr/share/oscar/icones/vnc64.png">exec:vncviewer</TrayButton>
		<TrayButton popup="Firefox" icon="firefox.png">exec:firefox</TrayButton>
		<TrayButton popup="Terminal" icon="xterm.png">exec:terminal >/dev/null 2>&1</TrayButton>
		<TrayButton popup="Graveur CD/DVD" icon="xfburn.png">exec:xfburn</TrayButton>
		<TrayButton popup="Geany" icon="geany.png">exec:geany</TrayButton>
		<TrayButton popup="Epdfview" icon="/usr/share/oscar/icones/postscript-viewer.png">exec:epdfview</TrayButton>
		<TrayButton popup="GVim<" icon="vim.png">exec:gvim</TrayButton>

		<Pager-ignore/>
		<TaskList/>
		<Dock/>
		<Clock format="%H:%M ">xclock</Clock>
		<TrayButton popup="Arret" icon="/usr/share/oscar/icones/exit1.png">exec:killall jwm</TrayButton>
	</Tray>
	
	<IconPath>/root/jwm/icons/</IconPath>
	
	<Desktops count="4">
		<Background>Tan</Background>
	</Desktops>

	<!-- Double click speed (in milliseconds) -->
	<DoubleClickSpeed>400</DoubleClickSpeed>
	
	<!-- Double click delta (in pixels) -->
	<DoubleClickDelta>2</DoubleClickDelta>
	
	<!-- The focus model (sloppy or click) -->
	<FocusModel>click</FocusModel>
	
	<!-- The snap mode (none, screen, or border) -->
	<SnapMode distance="10">border</SnapMode>
	
	<!-- The move mode (outline or opaque) -->
	<MoveMode>opaque</MoveMode>
	
	<!-- The resize mode (outline or opaque) -->
	<ResizeMode>opaque</ResizeMode>
	
	<!-- Key bindings -->
	<Key key="Up">up</Key>
	<Key key="Down">down</Key>
	<Key key="Right">right</Key>
	<Key key="Left">left</Key>
	<Key key="h">left</Key>
	<Key key="j">down</Key>
	<Key key="k">up</Key>
	<Key key="l">right</Key>
	<Key key="Return">select</Key>
	<Key key="Escape">escape</Key>
	
	<Key mask="A" key="Tab">next</Key>
	<Key mask="A" key="F4">close</Key>
	<Key mask="A" key="#">desktop#</Key>
	<Key mask="A" key="F1">root:1</Key>
	<Key mask="A" key="F2">window</Key>
	
	<Key mask="SC" key="s">shade</Key>
	<Key mask="SC" key="x">close</Key>
	<Key mask="SC" key="m">maximize</Key>
	<Key mask="SC" key="v">minimize</Key>
	<Key mask="SC" key="d">desktop</Key>
	<Key mask="SC" key="b">resize</Key>
	<Key mask="SC" key="z">root:1</Key>
	<Key mask="SC" key="n">move</Key>
	
	<Key mask="CA" key="r">restart</Key>
	<Key mask="CA" key="Return">exec:aterm</Key>
	<Key mask="CA" key="v">exec:aterm -T "vim" -e vi</Key>
	
	<!-- Visual Styles -->
	<WindowStyle>
		<Font antialias="false">-*-helvetica-medium-r-*-*-12-*-*-*-*-*-iso8859-1</Font>
		<Width>4</Width>
		<Height>20</Height>
		<Active>
			<Text>white</Text>
			<Title>grey40:grey0</Title>
			<Corner>grey44</Corner>
			<Outline>grey13</Outline>
		</Active>
		<Inactive>
			<Text>grey44</Text>
			<Title>grey20:grey0</Title>
			<Corner>grey44</Corner>
			<Outline>grey13</Outline>
		</Inactive>
	</WindowStyle>

	<TrayStyle>
		<Font antialias="false">-*-helvetica-medium-r-*-*-12-*-*-*-*-*-iso8859-1</Font>
		<Background>grey13</Background>
		<Foreground>white</Foreground>
	</TrayStyle>

	<TaskListStyle>
		<Font antialias="false">-*-helvetica-medium-r-*-*-12-*-*-*-*-*-iso8859-1</Font>
		<ActiveForeground>white</ActiveForeground>
		<ActiveBackground>grey50:grey30</ActiveBackground>
		<Foreground>grey70</Foreground>
		<Background>grey50:grey0</Background>
	</TaskListStyle>

<StartupCommand>killall terminal </StartupCommand>
<StartupCommand>killall xterm </StartupCommand>
</JWM>
